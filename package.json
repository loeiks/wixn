{
  "name": "wixn",
  "version": "1.4.0",
  "description": "Basic tool to install, update and uninstall multiple NPM packages at the same time with `wix` CLI.",
  "bin": {
    "wixn": "./index.js"
  },
  "keywords": [
    "loeiks",
    "wix",
    "wix install",
    "wix-install",
    "wix npm"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git@github.com:loeiks/wixn.git",
    "type": "git"
  },
  "contributors": [
    {
      "name": "Enes Bekar",
      "url": "https://github.com/loeiks"
    }
  ],
  "dependencies": {
    "chalk": "^5.6.0",
    "execa": "^9.6.0",
    "lodash": "^4.17.21",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.1",
    "terser": "^5.44.0",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "tsc && terser ./index.js -o ./index.js"
  }
}