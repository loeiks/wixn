{
  "name": "wixn",
  "version": "1.4.2",
  "description": "Basic tool to install, update and uninstall multiple NPM packages at the same time with `wix` CLI.",
  "bin": {
    "wixn": "./index.js"
  },
  "keywords": [
    "loeiks",
    "wix",
    "wix install",
    "wix-install",
    "wix npm"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git@github.com:loeiks/wixn.git",
    "type": "git"
  },
  "contributors": [
    {
      "name": "Enes Bekar",
      "url": "https://github.com/loeiks"
    }
  ],
  "dependencies": {
    "chalk": "^5.6.2",
    "execa": "^9.6.1",
    "lodash": "^4.17.23",
    "ora": "^9.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.23",
    "@types/node": "^20.19.31",
    "terser": "^5.46.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc && terser ./index.js -o ./index.js"
  }
}